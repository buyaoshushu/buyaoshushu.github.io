---
# Remember to set production_url in your _config.yml file!
title : Sitemap
---
<?xml version="1.0" encoding="utf-8"?>
<urlset>
{% for page in site.pages %}
    <url>
        <loc>{{site.production_url}}{{ page.url }}</loc>
    </url>
{% endfor %}{% for post in site.posts %}
    <url>
        <loc>{{site.production_url}}{{ page.url }}</loc>
    </url>
{% endfor %}
</urlset>
